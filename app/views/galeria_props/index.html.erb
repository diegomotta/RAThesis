<h1>Listing galeria_props</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
  <% if @prop_general.galeria_props.empty? == false%>
    <% @galeria_props.each do |galeria_prop| %>
      <tr>
      <td><%= image_tag galeria_prop.image.url(:thumb) %></td>
        <td><%= link_to 'Show', empresa_producto_prop_general_galeria_prop_path(@empresa,@producto,@prop_general,galeria_prop)%></td>
        <td><%= link_to 'Edit', edit_empresa_producto_prop_general_galeria_prop_path(@empresa,@producto,@prop_general,galeria_prop) %></td>
        <td><%= link_to 'Destroy', empresa_producto_prop_general_galeria_prop_path(@empresa,@producto,@prop_general,galeria_prop), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo', new_empresa_producto_prop_general_galeria_prop_path %>
<%= link_to 'Atras', empresa_producto_prop_generals_path %>
