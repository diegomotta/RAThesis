<h1>Listing galeria_prep_mates</h1>

<table>
  <thead>
    <tr>
      <th>Título</th>
      <th>Descrición</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @galeria_prep_mates.each do |galeria_prep_mate| %>
      <tr>
        <td><%= galeria_prep_mate.título %></td>
        <td><%= galeria_prep_mate.descrición %></td>
         <td><%= image_tag galeria_prep_mate.image.url(:thumb) %></td>
        <td><%= link_to 'Show', galeria_prep_mate %></td>
        <td><%= link_to 'Edit', edit_galeria_prep_mate_path(galeria_prep_mate) %></td>
        <td><%= link_to 'Destroy', galeria_prep_mate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Galeria prep mate', new_galeria_prep_mate_path %>
